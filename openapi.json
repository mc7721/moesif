  /users:
    post:
      summary: Update a User
      operationId: UpdateAUser
      tags:
        - Users
      parameters:
        - name: user_id
          in: query
          description: The unique identifier for this user.
          required: true
          schema:
            type: string
        - name: company_id
          in: query
          description: Associate the user with a company (Helpful for B2B companies)
          required: false
          schema:
            type: string
        - name: session_token
          in: query
          description: 'Associate this user with a new API key/session token. This field is append only meaning when you set this field, previously set tokens are not removed.'
          required: false
          schema:
            type: string
        - name: modified_time
          in: query
          description: Last modified time of user profile. Set automatically by Moesif if not provided.
          required: false
          schema:
            type: string
        - name: ip_address
          in: query
          description: Set the user’s last known ip address. Moesif sets this automatically from the user’s most recent API activity if not provided.
          required: false
          schema:
            type: string
        - name: user_agent_string
          in: query
          description: Set the user’s last known user agent. Moesif sets this automatically from the user’s most recent API activity if not provided.
          required: false
          schema:
            type: string
        - name: campaign
          in: query
          description: 'Referrer and UTM parameters to track effectiveness of your acquisition channels. Set automatically by moesif-browser-js, but not with server side SDKs'
          required: false
          schema:
            type: object
        - name: utm_source
          in: query
          description: UTM parameter that identifies which site sent the traffic
          required: false
          schema:
            type: string
        - name: utm_medium
          in: query
          description: 'UTM parameter that identifies what type of link was used, such as cost per click or email.'
          required: false
          schema:
            type: string
        - name: utm_campaign
          in: query
          description: UTM parameter that identifies a specific product promotion or strategic campaign.
          required: false
          schema:
            type: string
        - name: utm_term
          in: query
          description: UTM parameter that identifies search terms.
          required: false
          schema:
            type: string
        - name: utm_content
          in: query
          description: 'UTM parameter that identifies what specifically was clicked to bring the user to the site, such as a banner ad or a text link.'
          required: false
          schema:
            type: string
        - name: referrer
          in: query
          description: The referring URI before your domain.
          required: false
          schema:
            type: string
        - name: referring_domain
          in: query
          description: The referring domain of the page that linked to your domain.
          required: false
          schema:
            type: string
        - name: gclid
          in: query
          description: Google click Identifier to track Google Ads
          required: false
          schema:
            type: string
        - name: metadata
          in: query
          description: An object containing user demographics or other properties you want to store with this profile.
          required: false
          schema:
            type: object
      responses:
        '200':
          description: 'Successful Response'
