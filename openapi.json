openapi: 3.0.0
info:
  version: 1.0.0
  title: Moesif Management API
servers:
  - url: 'http:/localhost:3000'
paths:
  /actions:
    post:
      summary: Track a User Action
      operationId: TrackAUserAction
      tags:
        - Actions
      parameters:
        - name: action_name
          in: query
          description: A recognizable name such as Clicked Sign Up or Purchased Subscription
          required: true
          schema:
            type: string
        - name: session_token
          in: query
          description: The customer’s current session token as a string.
          required: false
          schema:
            type: string
        - name: user_id
          in: query
          description: The user identifier to associate this action with.
          required: false
          schema:
            type: string
        - name: company_id
          in: query
          description: The company identifier to associate this action with (Helpful for B2B companies).
          required: false
          schema:
            type: string
        - name: metadata
          in: query
          description: An object containing any custom event metadata you want to store with this event.
          required: false
          schema:
            type: object
        - name: request
          in: query
          description: The object containing the action’s request context.
          required: true
          schema:
            type: object
        - name: time
          in: query
          description: Timestamp for the action in ISO 8601 format. Set by server automatically if not set.
          required: false
          schema:
            type: string
        - name: uri
          in: query
          description: 'Full uri customer is on such as https:/wwww.acmeinc.com/pricing?utm_source=adwords including protocol, host, and query string.'
          required: true
          schema:
            type: string
        - name: ip_address
          in: query
          description: 'IP address of the customer, If not set, we extract the remote IP address.'
          required: false
          schema:
            type: string
        - name: user_agent_string
          in: query
          description: The customer’s browser agent string for device context.
          required: false
          schema:
            type: string
      responses:
        '200':
          description: 'Successful Response'
